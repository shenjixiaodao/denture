<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuzi.denture.data.factory.mapper.DentureMapper" >

    <insert id="save" parameterType="com.yuzi.denture.domain.factory.Denture">
        INSERT INTO denture
        (
        `id`,
        `order_id`,
        `factory_id`
        )
        VALUES
        (
        #{id},
        #{orderId},
        #{factoryId}
        );
    </insert>

</mapper>